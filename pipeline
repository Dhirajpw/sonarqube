pipeline {
    agent any
    stages { 
        stage('pull') {
            steps {
                echo "successfull pull form git"
                git 'https://github.com/deepak-umre/studentapp.ui.git'
            }
        }
        stage ('build') {
            steps {
                echo "build by maven"
                sh 'mvn clean package'
            }

            }
        stage ('test') {
            steps {
                echo "test by sonar"
                withSonarQubeEnv(installationName: 'sonar' , credentialsId: 'sonar_token') {
                    sh '''mvn sonar:sonar \\
                    -Dsonar.projectKey=my-project \\
                              

                
                }
            }
        }
    }
}