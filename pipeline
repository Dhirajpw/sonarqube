pipeline {
    agent any
    stages {
        stage('Pull') {
            steps {
                echo "Successful pull from Git"
                git 'https://github.com/deepak-umre/studentapp.ui.git'
            }
        }
        stage('Build') {
            steps {
                echo "Building with Maven"
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo "test is getting successfull by sonarqube"
                withSonarQubeEnv(installationName: 'sonar' , credentialsId: 'sonar_token') {
                sh 'mvn sonar:sonar -Dsonar.projectKey=studentapp'  

                    
                }
            }
        }
    }
}
